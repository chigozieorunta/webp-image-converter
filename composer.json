{
    "name": "chigozieorunta/webp-image-converter",
    "description": "A simple WP plugin to help convert JPG/PNG images during page runtime to WebP formats.",
    "type": "wordpress-plugin",
    "license": "GPL-2.0-or-later",
    "autoload": {
        "psr-4": {
            "WebPImageConverter\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Chigozie Orunta",
            "email": "chigozieorunta@yahoo.com"
        }
    ],
    "require-dev": {
        "dealerdirect/phpcodesniffer-composer-installer": "^1.0",
        "wp-coding-standards/wpcs": "^2.3",
        "codeception/codeception": "^5.0",
        "codeception/module-phpbrowser": "*",
        "codeception/module-asserts": "*"
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
